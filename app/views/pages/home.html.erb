                                <div class="col-md-6">
                                    <div class="input-box">
                                        <input type="text" name="form_name" value="" placeholder="Your Name" required="">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="input-box">
                                        <input type="email" name="form_email" value="" placeholder="Your Email" required="">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="input-box">
                                        <select class="selectmenu">
                                            <option selected="selected">Select Service</option>
                                            <option>Massage Therapy</option>
                                            <option>Facial Treatments</option>
                                            <option>Pedicure & Menicure</option>
                                            <option>Salt and Oil Bath</option>
                                            <option>Perfect Makeup</option>
                                            <option>Hair Treatments</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <textarea name="form_message" placeholder="Your Message.." required=""></textarea>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <button class="thm-btn bgclr-1" type="submit">submit</button>   
                                </div>
                            </div>
                        </form>  
                    </div>

                </div>
            </div>
            
            <div class="col-md-3">
